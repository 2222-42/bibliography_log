{
  "run": {
    "modules-download-mode": "readonly"
  },
  "linters": {
    "enable": [
      "gofmt",
      "goimports",
      "misspell",
      "revive"
    ]
  },
  "linters-settings": {
    "goimports": {
      "local-prefixes": "bibliography_log"
    }
  },
  "issues": {
    "exclude-dirs": [
      "third_party",
      "builtin",
      "examples"
    ]
  }
}