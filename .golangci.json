{
  "version": "2",
  "formatters": {
    "enable": [
      "gofmt",
      "goimports"
    ],
    "settings": {
      "goimports": {
        "local-prefixes": [
          "bibliography_log"
        ]
      }
    }
  },
  "linters": {
    "enable": [
      "misspell",
      "revive"
    ]
  },
  "run": {
    "modules-download-mode": "readonly",
    "timeout": "5m"
  }
}
